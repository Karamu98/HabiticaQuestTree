{ 
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "moduleResolution": "node",
      "sourceMap": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "removeComments": false,
      "noImplicitAny": false,
      "suppressImplicitAnyIndexErrors": true,
      "outDir": "obj",
      "noEmit": false,
      "esModuleInterop": true
    },
    "exclude": [ "node_modules" ] 
}